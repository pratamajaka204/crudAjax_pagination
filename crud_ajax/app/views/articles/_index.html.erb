<% @article.each do |article| %>
  <tr>
  	
    <td><%= article.title %></td>
    <td><%= article.content %></td>
    <td><%= article.status %></td>
    <td><%= link_to "Edit", edit_article_path(article), remote: true, class: "btn btn-success" %></td>
    <td><%= link_to "Delete", article_delete_path(article), remote: true, class: "btn btn-danger" %></td>
    </tr>


<% end %>

<tr><td colspan="5"><%= paginate @article %></td></tr>
